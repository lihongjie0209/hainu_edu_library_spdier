	#showVisitedCountDiv{
		text-align:right;
		padding-right:20px;
		margin-top:26px;
	}
</style>
<script type="text/javascript">
<!--
$(function() {
	var $navPageBaseURL = $("#navPageBaseURL").text();
	if($navPageBaseURL != "") {
		$("#reSearchButton").show();
	}
	
	if("1" == "1")
	{
		if("false" == "true")
		{
