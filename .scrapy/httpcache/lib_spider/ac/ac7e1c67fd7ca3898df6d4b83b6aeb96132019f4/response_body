<link type="text/css" href="/opac/media/style/default/space.css;jsessionid=5FDC018622474BA2CB58A47A30692B83" rel="stylesheet" />
<style type="text/css">
	#showVisitedCountDiv{
		text-align:right;
		padding-right:20px;
		margin-top:26px;
	}
</style>
<script type="text/javascript">
<!--
$(function() {
	var $navPageBaseURL = $("#navPageBaseURL").text();
	if($navPageBaseURL != "") {
		$("#reSearchButton").show();
	}
	
	if("1" == "1")
	{
		if("false" == "true")
		{
			$("#baseSearchForm select[name='searchWay']").val("");
			return;
		}
		if("false"=="true")
		{
			$("#baseSearchForm select[name='searchWay']").val("");
