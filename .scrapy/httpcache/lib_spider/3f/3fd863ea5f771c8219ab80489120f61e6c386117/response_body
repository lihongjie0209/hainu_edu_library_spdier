<link type="text/css" href="/opac/media/style/default/space.css;jsessionid=EBBB5292D697C9224C3AC22CEE8E7AE9" rel="stylesheet" />
<style type="text/css">
	#showVisitedCountDiv{
		text-align:right;
		padding-right:20px;
		margin-top:26px;
	}
</style>
<script type="text/javascript">
<!--
$(function() {
	var $navPageBaseURL = $("#navPageBaseURL").text();
	if($navPageBaseURL != "") {
		$("#reSearchButton").show();
	}
	
	if("1" == "1")
	{
		if("false" == "true")
		{
			$("#baseSearchForm select[name='searchWay']").val("");
			return;
		}
		if("false"=="true")
